#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include "enemy.h"

void enemycreat(struct Enemy *eobj){
	eobj->x = 0;
	eobj->y = 0;
	eobj->Xmotion = 0;
	eobj->Ymotion = 0;
	eobj->lifetime = 0;
	eobj->bulletSwitch = 0;
	eobj->bulletType = 0;
	eobj->HP = 20;
	eobj->collideSize = 0;
	eobj->actionFrame = 0;
	eobj->collideSize = 5;
}

