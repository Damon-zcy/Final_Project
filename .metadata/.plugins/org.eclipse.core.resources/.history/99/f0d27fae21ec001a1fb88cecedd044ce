#ifndef SRAM_H_
#define SRAM_H_
#include "alt_types.h"
#include "system.h"

// These addresses are defined in system.h generated by Eclipse, and they
// should have been matched to the addresses shown in Qsys.
// If for any reason they are not correctly defined, you can replace these
// with the addresses you read in Qsys.

#define sram_base				(volatile char*)	SRAM_0_BASE
#define request_base			(volatile char*)	DATA_REQUEST_BASE
#define pic_base				(volatile char*)	PICK_DATA_BASE
	unsigned char* sram = sram_base;
	unsigned int* s = DATA_REQUEST_BASE;

	volatile unsigned char* s2 = PICK_DATA_BASE;

void sram_init(void);


#endif
