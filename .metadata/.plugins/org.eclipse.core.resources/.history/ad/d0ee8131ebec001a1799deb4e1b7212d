#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include "enemy.h"

void enemycreat(struct Player *pobj){
	pobj->x = 0;
	pobj->y = 0;
	pobj->Xmotion = 0;
	pobj->Ymotion = 0;
	pobj->lifetime = 0;
	pobj->bulletSwitch = 0;
	pobj->bulletType = 0;
	pobj->HP = 20;
	pobj->collideSize = 0;
	pobj->actionFrame = 0;
	pobj->collideSize = 5;
}

void enext(){
	player.lifetime++;
	player.x += player.Xmotion;
	player.y += player.Ymotion;

};

